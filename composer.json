{
    "name": "raffaelecarelle/symfony-pro-backup-bundle",
    "description": "Symfony bundle for database and filesystem backup/restore management",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffaele Carelle",
            "email": "raffaele.carelle@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "dragonmantank/cron-expression": "^3.3",
        "symfony/config": "^6.4|^7.0|^8.0",
        "symfony/console": "^6.4|^7.0|^8.0",
        "symfony/dependency-injection": "^6.4|^7.0|^8.0",
        "symfony/event-dispatcher": "^6.4|^7.0|^8.0",
        "symfony/framework-bundle": "^6.4|^7.0|^8.0",
        "symfony/http-foundation": "^6.4|^7.0|^8.0",
        "symfony/http-kernel": "^6.4|^7.0|^8.0",
        "symfony/messenger": "^6.4|^7.0|^8.0",
        "symfony/process": "^6.4|^7.0|^8.0",
        "symfony/scheduler": "^6.4|^7.0|^8.0",
        "symfony/serializer": "^6.4|^7.0|^8.0",
        "symfony/yaml": "^6.4|^7.0|^8.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "aws/aws-sdk-php": "*",
        "doctrine/doctrine-bundle": "^2.0|^3.0",
        "friendsofphp/php-cs-fixer": "^3.85",
        "google/cloud-storage": "*",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.11|^2.0",
        "phpstan/phpstan-symfony": "^1.4|^2.0",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^2.1",
        "symfony/browser-kit": "^6.4|^7.0|^8.0",
        "symfony/css-selector": "^6.4|^7.0|^8.0",
        "symfony/dotenv": "^6.4|^7.0|^8.0",
        "symfony/phpunit-bridge": "^6.4|^7.0|^8.0",
        "symfony/twig-bundle": "^6.4|^7.0|^8.0",
        "symfony/web-profiler-bundle": "^6.4|^7.0|^8.0"
    },
    "suggest": {
        "aws/aws-sdk-php": "For S3 storage adapter",
        "google/cloud-storage": "For Google Cloud storage adapter",
        "doctrine/doctrine-bundle": "For database backup support"
    },
    "autoload": {
        "psr-4": {
            "ProBackupBundle\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
      "ProBackupBundle\\Tests\\": "tests/",
      "TestApp\\": "tests/_fixtures/TestApp/src/"
    }
  },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
