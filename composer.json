{
    "name": "raffaelecarelle/symfony-backup-bundle",
    "description": "Symfony bundle for database and filesystem backup/restore management",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffaele Carelle",
            "email": "raffaele.carelle@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "dragonmantank/cron-expression": "^3.3",
        "symfony/config": "5.4.*|6.4.*|^7.0",
        "symfony/console": "5.4.*|6.4.*|^7.0",
        "symfony/dependency-injection": "5.4.*|6.4.*|^7.0",
        "symfony/event-dispatcher": "5.4.*|6.4.*|^7.0",
        "symfony/framework-bundle": "5.4.*|6.4.*|^7.0",
        "symfony/http-foundation": "5.4.*|6.4.*|^7.0",
        "symfony/http-kernel": "5.4.*|6.4.*|^7.0",
        "symfony/messenger": "^7.3",
        "symfony/process": "5.4.*|6.4.*|^7.0",
        "symfony/scheduler": "^7.3",
        "symfony/yaml": "5.4.*|6.4.*|^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.85",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^2.1",
        "symfony/browser-kit": "5.4.*|6.4.*|^7.0",
        "symfony/css-selector": "5.4.*|6.4.*|^7.0",
        "symfony/phpunit-bridge": "5.4.*|6.4.*|^7.0",
        "symfony/twig-bundle": "5.4.*|6.4.*|^7.0",
        "symfony/web-profiler-bundle": "5.4.*|6.4.*|^7.0",
        "ext-pdo": "*"
    },
    "suggest": {
        "aws/aws-sdk-php": "For S3 storage adapter",
        "google/cloud-storage": "For Google Cloud storage adapter",
        "doctrine/doctrine-bundle": "For database backup support"
    },
    "autoload": {
        "psr-4": {
            "ProBackupBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProBackupBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
